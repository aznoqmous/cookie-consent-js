{
    "functionnal": {
        "type": "functionnal",
        "mandatory": true
    },
    "google": {
        "type": "statistic",
        "scripts" : [
            "google",
            "gtag"
        ],
        "cookies": [
            "^__utm",
            "^_ga",
            "^_gid"
        ]
    },
    "gtm": {
        "type": "api",
        "scripts": [
            "googletagmanager"
        ],
        "cookies": [
            "^_gcl_au",
            "^_gat_"
        ]
    },
    "facebook": {
        "type": "statistic",
        "scripts": [
            "fbq",
            "facebook"
        ],
        "cookies": [
            "^_fb",
            "^datr",
            "^fr",
            "^reg_ext_ref",
            "^reg_fb_gate",
            "^reg_fb_ref",
            "^sb",
            "^wd",
            "^x-src"
        ]
    },
    "youtube": {
        "type": "video",
        "iframes": [
            "youtube.com"
        ]
    },
    "vimeo": {
        "type": "video",
        "iframes": [
            "vimeo.com"
        ]
    }
}